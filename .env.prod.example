# Production Environment Variables
# Copy this file to .env.prod and fill in your actual values

# Database Configuration
POSTGRES_PASSWORD=your_secure_database_password_here

# AI API Keys (Required)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub Webhook Security (Optional)
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here

# Domain Configuration (if using reverse proxy/domain)
DOMAIN=localhost
API_URL=http://localhost:8002
WS_URL=ws://localhost:8002/ws

# Additional Security
JWT_SECRET_KEY=your_jwt_secret_for_future_auth
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com